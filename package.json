{
  "name": "smart-dashboard",
  "version": "0.1.10",
  "description": "A react base dashboard engine",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "doc": "typedoc --options typedoc.json",
    "start": "build-scripts start",
    "build": "build-scripts build",
    "test": "echo \"no test\"",
    "postinstall": "husky install && npm run initHusky",
    "prepublishOnly": "npm run lint && npm run build",
    "initHusky": "git add .husky/pre-commit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint",
    "eslint": "eslint --cache --ext .js,.jsx,.tsx,.ts ./",
    "eslint:fix": "eslint --cache --fix --ext .js,.jsx,.tsx,.ts ./",
    "stylelint": "stylelint --allow-empty-input ./**/*.scss",
    "prettier": "prettier --write \"./{src,demo}/**/*.{tsx,js,jsx,json,css,ts,scss,less}\""
  },
  "author": "bruce007lee",
  "license": "MIT",
  "keywords": [
    "react",
    "dashboard"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:bruce007lee/smart-dashboard.git"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-rnd": "^10.3.7"
  },
  "devDependencies": {
    "@ant-design/plots": "^1.0.9",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "antd": "^4.21.4",
    "build-plugin-component": "^1.6.5",
    "build-plugin-fusion": "^0.1.0",
    "build-plugin-moment-locales": "^0.1.0",
    "build-scripts": "^1.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^26.4.2",
    "prettier": "^2.0.5",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "stylelint": "^14.9.1",
    "typedoc": "~0.20.25",
    "typedoc-plugin-markdown": "~3.5.0",
    "typescript": "^4.1.6",
    "webpack": "^4.27.1",
    "webpack-dev-server": "^3.7.2"
  },
  "files": [
    "/lib",
    "/es",
    "/build"
  ],
  "homepage": "https://unpkg.com/smart-dashboard@0.1.10/build/index.html"
}
